                if(form.$invalid) return;
                
                var data = appraisalService.get();
                data.address = vm.address;
                data.city = vm.city;
                data.manualAddress = true;
                appraisalService.set(data);
                vm.stepComplete();
            }

        }],
        bindToController: true,
        controllerAs: 'vm'
    }
}]);
