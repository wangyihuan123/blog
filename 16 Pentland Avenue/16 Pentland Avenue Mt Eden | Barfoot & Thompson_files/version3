    'btBranchAbout', ['$scope', 'textExpander', function ($scope, textExpander) {
            
            var init = function() {
                textExpander.init('branch-about-text', 'branchAboutBtn', 160);
            }

            $scope.showDescription = function() {
                textExpander.animate('branch-about-text');
            };

            init();
        }
    ]);
